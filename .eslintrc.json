{
    "rules": {
        "indent": [
            1,
            2
        ],
        "quotes": [
            1,
            "single"
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "semi": [
            1,
            "always"
        ],
        "no-unused-vars" : 0,
        "no-undef" : 0
    },
    "env": {
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
